<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RabbitDashboardActivity"
    android:orientation="vertical">

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/refreshSwipeLayout">

        <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_rowWeight="1"
                    android:layout_columnWeight="1"
                    android:elevation="6dp"
                    app:cardBackgroundColor="@color/dashboard_header"
                    app:cardCornerRadius="12dp"
                    android:layout_marginTop="20dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:padding="6dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="WM Conejo El Patio"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:textSize="30sp" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_rowWeight="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="12dp"
                    android:elevation="6dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="12dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:padding="6dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Total Rabbits"
                            android:textColor="@color/gray"
                            android:textSize="25sp"
                            app:drawableLeftCompat="@drawable/rabbit_icon"
                            />

                        <TextView
                            android:id="@+id/tvTotalRabbit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textSize="30sp" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_rowWeight="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="12dp"
                    android:elevation="6dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="12dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:padding="6dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Total Kits"
                            android:textColor="@color/gray"
                            android:textSize="25sp"
                            app:drawableLeftCompat="@drawable/rabbit_icon"/>

                        <TextView
                            android:id="@+id/tvTotalKits"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textSize="30sp" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_rowWeight="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="12dp"
                    android:elevation="6dp"
                    app:cardBackgroundColor="@color/white">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:padding="15dp">


                        <TextView
                            android:id="@+id/tvRabbitDeceasedTotal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Deceased Rabbit: 0"
                            android:textColor="@color/gray"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/tvKitsDeceasedTotal"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Deceased Kits: 0"
                            android:textColor="@color/gray"
                            android:textSize="15sp"
                            android:gravity="right"/>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <GridLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:alignmentMode="alignMargins"
                    android:columnCount="2"
                    android:rowCount="2">

                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_rowWeight="1"
                        android:layout_columnWeight="1"
                        android:layout_margin="12dp"
                        android:elevation="6dp"
                        app:cardCornerRadius="12dp"
                        app:cardBackgroundColor="@color/dashboard_header">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="6dp">


                            <TextView
                                android:id="@+id/tvBuckCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textSize="30sp"
                                android:textColor="@color/white"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Total Buck "
                                android:textStyle="bold"
                                android:textSize="20sp"
                                android:textColor="@color/white"
                                />

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_rowWeight="1"
                        android:layout_columnWeight="1"
                        android:layout_margin="12dp"
                        android:elevation="6dp"
                        app:cardCornerRadius="12dp"
                        app:cardBackgroundColor="@color/pink">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="6dp">

                            <TextView
                                android:id="@+id/tvDoeCount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textColor="@color/white"
                                android:textSize="30sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Total Doe"
                                android:textColor="@color/white"
                                android:textStyle="bold"
                                android:textSize="20sp" />
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>


                </GridLayout>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_rowWeight="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="12dp"
                    android:elevation="6dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="12dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:padding="6dp">

                        <TextView
                            android:id="@+id/tvTotalDoe"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Total Doe: "
                            android:textColor="@color/gray"
                            android:textSize="20sp"
                            android:gravity="center"
                            android:textAlignment="center"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:padding="6dp">
                            <TextView
                                android:id="@+id/tvPregnantTotal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Pregnant: "
                                android:textColor="@color/black"
                                android:textSize="15sp" />

                            <TextView
                                android:id="@+id/tvNotPregnantTotal"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:text="Not Pregnant: "
                                android:textColor="@color/black"
                                android:textSize="15sp"
                                android:gravity="right"/>
                        </LinearLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_rowWeight="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="12dp"
                    android:elevation="6dp"
                    app:cardBackgroundColor="@color/dashboard_header"
                    app:cardCornerRadius="12dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:padding="6dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="BREEDS"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:textSize="25sp" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <androidx.cardview.widget.CardView
                        android:layout_width="180dp"
                        android:layout_height="wrap_content"
                        android:layout_rowWeight="1"
                        android:layout_columnWeight="1"
                        android:layout_margin="12dp"
                        android:elevation="6dp"
                        app:cardBackgroundColor="@color/white"
                        app:cardCornerRadius="12dp"
                        >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="New Zealand"
                                    android:textColor="@color/black"
                                    android:textSize="17sp"
                                    android:layout_marginBottom="10dp"
                                    />

                                <TextView
                                    android:id="@+id/tvNewZealandBreed"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/pastel_light_blue"
                                    android:textSize="25sp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="right"
                                    android:textStyle="bold"
                                    />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Californian"
                                    android:textColor="@color/black"
                                    android:textSize="17sp"
                                    android:layout_marginBottom="10dp"
                                    />

                                <TextView
                                    android:id="@+id/tvCalifornianBreed"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/pastel_light_blue"
                                    android:textSize="25sp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="right"
                                    android:textStyle="bold"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Flemish Giant"
                                    android:textColor="@color/black"
                                    android:textSize="17sp"
                                    android:layout_marginBottom="10dp"
                                    />

                                <TextView
                                    android:id="@+id/tvFlemishGiantBreed"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/pastel_light_blue"
                                    android:textSize="25sp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="right"
                                    android:textStyle="bold"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="PS Line"
                                    android:textColor="@color/black"
                                    android:textSize="17sp"
                                    android:layout_marginBottom="10dp"
                                    />

                                <TextView
                                    android:id="@+id/tvPsLineBreed"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/pastel_light_blue"
                                    android:textSize="25sp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="right"
                                    android:textStyle="bold"
                                    />
                            </LinearLayout>

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="180dp"
                        android:layout_height="wrap_content"
                        android:layout_rowWeight="1"
                        android:layout_columnWeight="1"
                        android:layout_margin="12dp"
                        android:elevation="6dp"
                        app:cardBackgroundColor="@color/white"
                        app:cardCornerRadius="12dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:padding="10dp">



                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Hyla Optima"
                                    android:textColor="@color/black"
                                    android:textSize="17sp"
                                    android:layout_marginBottom="10dp"
                                    />

                                <TextView
                                    android:id="@+id/tvHylaOptimaBreed"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/pastel_light_blue"
                                    android:textSize="25sp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="right"
                                    android:textStyle="bold"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Hyla Plus"
                                    android:textColor="@color/black"
                                    android:textSize="17sp"
                                    android:layout_marginBottom="10dp"
                                    />

                                <TextView
                                    android:id="@+id/tvHylaPlusBreed"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/pastel_light_blue"
                                    android:textSize="25sp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="right"
                                    android:textStyle="bold"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="German Giant"
                                    android:textColor="@color/black"
                                    android:textSize="17sp"
                                    android:layout_marginBottom="10dp"
                                    />
                                <TextView
                                    android:id="@+id/tvGermanGiantBreed"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/pastel_light_blue"
                                    android:textSize="25sp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="right"
                                    android:textStyle="bold"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Holland Lop"
                                    android:textColor="@color/black"
                                    android:textSize="17sp"
                                    android:layout_marginBottom="10dp"
                                    />
                                <TextView
                                    android:id="@+id/tvHollandLopBreed"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/pastel_light_blue"
                                    android:textSize="25sp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="right"
                                    android:textStyle="bold"
                                    />
                            </LinearLayout>

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </LinearLayout>


                <!-- Buck Table -->
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_rowWeight="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="12dp"
                    android:elevation="6dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="12dp"
                    android:visibility="gone">

                    <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/white"
                        android:layout_margin="1dp">

                        <!-- Table Heading -->

                        <TableRow
                            android:background="@color/pastel_light_blue">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="Top Buck Breeders"
                                android:textStyle="bold"
                                android:textColor="@color/white"
                                android:padding="10sp"
                                android:textSize="20sp"
                                android:layout_weight="3"
                                android:gravity="center_horizontal"
                                />


                        </TableRow>

                        <TableRow
                            android:background="@color/green">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="Name"
                                android:textColor="@color/white"
                                android:padding="10sp"
                                android:textSize="14sp"
                                android:layout_weight="3"
                                android:gravity="center_horizontal"
                                />


                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="Success Rate"
                                android:textColor="@android:color/white"
                                android:padding="10sp"
                                android:textSize="14sp"
                                android:layout_weight="4"
                                android:gravity="center_horizontal"
                                />


                        </TableRow>

                        <!-- Table Data -->

                        <TableRow>

                            <TextView
                                android:id="@+id/buckRow1Name"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row1Name"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="3"
                                android:gravity="center_horizontal"
                                />



                            <TextView
                                android:id="@+id/buckRow1SuccessRate"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row1Surname"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="4"
                                android:gravity="center_horizontal"
                                />

                        </TableRow>
                        <TableRow
                            android:background="@color/light_gray">

                            <TextView
                                android:id="@+id/buckRow2Name"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row2Name"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="3"
                                android:gravity="center_horizontal"
                                />



                            <TextView
                                android:id="@+id/buckRow2SuccessRate"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row2Surname"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="4"
                                android:gravity="center_horizontal"
                                />

                        </TableRow>

                        <TableRow>

                            <TextView
                                android:id="@+id/buckRow3Name"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row3Name"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="3"
                                android:gravity="center_horizontal"
                                />



                            <TextView
                                android:id="@+id/buckRow3SuccessRate"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row3Surname"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="4"
                                android:gravity="center_horizontal"
                                />

                        </TableRow>

                        <TableRow android:background="@color/light_gray">

                            <TextView
                                android:id="@+id/buckRow4Name"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row3Name"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="3"
                                android:gravity="center_horizontal"
                                />

                            <TextView
                                android:id="@+id/buckRow4SuccessRate"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row3Surname"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="4"
                                android:gravity="center_horizontal"
                                />

                        </TableRow>

                        <TableRow>

                            <TextView
                                android:id="@+id/buckRow5Name"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row3Name"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="3"
                                android:gravity="center_horizontal"
                                />



                            <TextView
                                android:id="@+id/buckRow5SuccessRate"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/row3Surname"
                                android:padding="10sp"
                                android:textSize="12sp"
                                android:layout_weight="4"
                                android:gravity="center_horizontal"
                                />

                        </TableRow>

                    </TableLayout>

                </androidx.cardview.widget.CardView>

                <!-- Doe Table -->
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_rowWeight="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="12dp"
                    android:elevation="6dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="12dp"
                    android:visibility="gone">

                    <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/white"
                        android:layout_margin="1dp">

                        <!-- Table Heading -->

                        <TableRow
                            android:background="@color/pastel_light_blue">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="Top Doe Breeders"
                                android:textStyle="bold"
                                android:textColor="@color/white"
                                android:padding="10sp"
                                android:textSize="20sp"
                                android:layout_weight="3"
                                android:gravity="center_horizontal"
                                />


                        </TableRow>

                        <TableRow
                            android:background="@color/green">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="Name"
                                android:textColor="@color/white"
